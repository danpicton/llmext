{
  "manifest_version": 3,
  "name": "LLM Learn Pusher",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "prompt.html"
  },
  "icons": {
    "48": "brain-circuit.png"
  }
}
